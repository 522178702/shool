uni-page-body,
uni-page-refresh {
  height: 100% !important;
}
.startup-bg {
  width: 100%;
  height: 100%;
  background: url(/static/startup.png) no-repeat center;
  background-size: cover;
  position: relative;
}
.startup-bg .tips-time {
  position: absolute;
  right: 20rpx;
  top: 20rpx;
  padding: 2rpx 30rpx;
  border-radius: 25rpx;
  color: #fff;
  background-color: rgba(0, 0, 0, 0.1);
}

